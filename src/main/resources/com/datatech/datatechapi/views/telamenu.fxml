<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorpane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.datatech.datatechapi.controller.TelaMenuController">
   <children>
      <MenuBar fx:id="barramenu" layoutY="1.0" prefHeight="26.0" prefWidth="900.0">
        <menus>
          <Menu fx:id="menuprofessor" mnemonicParsing="false" text="Professor">
            <items>
              <MenuItem fx:id="mit_restricoes" mnemonicParsing="false" onAction="#cadastrarRestricoes" text="Restrições" />
            </items>
          </Menu>
          <Menu fx:id="menugrade" mnemonicParsing="false" text="Grade">
            <items>
              <MenuItem fx:id="mit_criargrade" mnemonicParsing="false" onAction="#criarGrade" text="Criar Grade" />
                  <MenuItem fx:id="mit_visualizargrade" mnemonicParsing="false" onAction="#visualizarGrade" text="Visualizar Grade" />
            </items>
          </Menu>
          <Menu fx:id="logout" mnemonicParsing="false" text="Sair">
            <items>
              <MenuItem fx:id="mit_logout" mnemonicParsing="false" onAction="#realizarLogout" text="Efetuar Logoff" />
                  <MenuItem fx:id="mit_sair" mnemonicParsing="false" onAction="#sairDoSistema" text="Sair do Sistema" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
